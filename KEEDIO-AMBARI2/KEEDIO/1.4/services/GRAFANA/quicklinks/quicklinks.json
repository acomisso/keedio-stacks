{
  "name": "default",
  "description": "default quick links configuration",
  "configuration": {
    "protocol":
    {
      "type":"http",
      "checks":[
        {
          "property":"protocol",
          "desired":"http,
          "site":"grafana.xml"
        }
      ]
    },
    "links": [
      {
        "name": "grafana",
        "label": "Grafana",
        "requires_user_name": "false",
        "component_name": "GRAFANA",
        "url":"%@://%@:%@",
        "port":{
          "http_property": "port",
          "http_default_port": "3000",
          "https_property": "port",
          "https_default_port": "3000",
          "regex": "^(\\d+)$",
          "site": "grafana.xml"
        }
      }
    ]
  }
}
