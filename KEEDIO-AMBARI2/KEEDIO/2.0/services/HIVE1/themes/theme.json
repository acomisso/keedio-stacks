        "config": "hive-env/hive_txn_acid",
        "widget": {
          "type": "toggle"
        }
      },
      {
        "config": "hive1-site/hive.compactor.initiator.on",
        "widget": {
          "type": "toggle"
        }
      },
      {
        "config": "hive1-site/hive.compactor.worker.threads",
