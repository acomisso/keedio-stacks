{
  "name": "default",
  "description": "default quick links configuration",
  "configuration": {
    "protocol":
    {
      "type":"HTTP_ONLY",
      "checks":[
        {
          "property":"protocol",
          "desired":"http",
          "site":"elasticsearch.xml"
        }
      ]
    },
    "links": [
      {
        "name": "cerebro",
        "label": "cerebro",
        "requires_user_name": "false",
        "component_name": "CEREBRO",
        "url":"%@://%@:%@",
        "port":{
          "http_property": "port",
          "http_default_port": "1234",
          "https_property": "port",
          "https_default_port": "1234",
          "regex": "^(\\d+)$",
          "site": "elasticsearch.xml"
        }
      }
    ]
  }
}
